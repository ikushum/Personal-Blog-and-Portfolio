<!---------------Blog section-->
<div id="blog">
<h1 class="header1" style="margin-bottom:30px;"> Latest Blog Posts </h1>
<% @posts.each do |post| %>
  <div id="post_list" style="text-align:center">
  <%= link_to post_path(post.id, post.title.parameterize.capitalize) do %>
  <div class="post_img"><img class="post_image" src="<%= post.postimg %>">  </div> 
  <h2 class="header1"><%= post.title %> </h2>
  <% end %>
  <h5 class="date_time">by <%= post.author %></h5>
  <h5 class="date_time"><%= post.created_at.to_s(:full_english) %></h5>
  
   <% if admin_signed_in? %>
    <a href="<%= edit_post_path(post.id) %>"><h5 class="edit_delete"> Edit Post </h5></a>
    <%= link_to post_path(post.id), method: 'delete', data: {confirm: 'Are you sure?'} do%>
      <h5 class="edit_delete"> Delete Post </h5>
    <% end %>
   <% end %> 
   
  <div id="button">
   <%= link_to post_path(post.id, post.title.parameterize.capitalize) do %>
    <text class="buttonDefault"> READ MORE </text>
   <% end %>
   </div>
   </div>
<% end %>
</div>

<!--Contact section-->
<div id="contact" class="section5">  <p class="services_p" style="color:red"> Under Construction </p>
<h1 class="header1">Contact Me</h1>
<p class="services_p" style="font-size:15px;padding:0px 20px;"> Want to say a friendly hello or hire me ? Go ahead and fill the form below. </p>
<div id="contact_wrapper">
 <%= simple_form_for Message.new, url: messages_path, method: :post do |f| %>
       <div class="input"> <%= f.input :name, label: false, placeholder: 'FULL NAME' %>  </div>
       <div class="input"> <%= f.input :email, label: false, placeholder: 'EMAIL' %>  </div>
       <div class="input"> <%= f.input :message, label: false, placeholder: 'MESSAGE' %>  </div>
    <div id="button" style="text-align:center;width:100%">
         <%= f.submit 'SEND MESSAGE', class: "submit_button", id: "submit" %>
    </div>
 <% end %>  
 </div>
